<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.jncc.entity.UserEntity" table="user" schema="school">
        <id name="id">
            <column name="id" sql-type="int(16)"/>
        </id>
        <property name="username">
            <column name="username" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>